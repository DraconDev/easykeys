{
    "name": "easykeys",
    "displayName": "easykeys",
    "description": "simple keys for navigation",
    "version": "1.0.0",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.85.0"
    },
    "publisher": "devdrac",
    "categories": [
        "Keymaps"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DraconDev?tab=repositories"
    },
    "contributes": {
        "keybindings": [
            {
                "key": "alt+e",
                "title": "cursorUp",
                "command": "cursorUp"
            },
            {
                "key": "alt+d",
                "title": "cursorDown",
                "command": "cursorDown"
            },
            {
                "key": "alt+f",
                "title": "cursorWordRight",
                "command": "cursorWordRight"
            },
            {
                "key": "alt+s",
                "title": "cursorWordLeft",
                "command": "cursorWordLeft"
            },
            {
                "key": "alt+r",
                "title": "cursorEnd",
                "command": "cursorEnd"
            },
            {
                "key": "alt+w",
                "title": "cursorHome",
                "command": "cursorHome"
            },
            {
                "key": "alt+x",
                "title": "cursorLeft",
                "command": "cursorLeft"
            },
            {
                "key": "alt+v",
                "title": "cursorRight",
                "command": "cursorRight"
            },
            {
                "key": "alt+3",
                "title": "cursorTop",
                "command": "cursorTop"
            },
            {
                "key": "alt+c",
                "title": "cursorBottom",
                "command": "cursorBottom"
            }
        ]
    }
}
